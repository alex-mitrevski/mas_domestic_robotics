<?xml version="1.0"?>
<package>
  <name>mdr_lwr_kinematics</name>
  <version>1.2.0</version>
  <description>
    This package provides forward kinematics and an analytical inverse
    kinematics solver for the Kuka LWR (Light-Weight Robot). The inverse
    kinematics solver is generated by IkFast [1].

    [1] http://openrave.org/docs/latest_stable/openravepy/ikfast/
  </description>
  <license>GPLv3</license>

  <maintainer email="robotics@inf.h-brs.de">MAS robotics</maintainer>
  <author email="sven.schneider@h-brs.de">Sven Schneider</author>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>cmake_modules</build_depend>
  <build_depend>moveit_core</build_depend>
  <build_depend>pluginlib</build_depend>
  <build_depend>roscpp</build_depend>
  <build_depend>tf_conversions</build_depend>
  <build_depend>rospy</build_depend>
  <build_depend>actionlib</build_depend>
  <build_depend>actionlib_msgs</build_depend>
  <build_depend>geometry_msgs</build_depend>

  <run_depend>moveit_core</run_depend>
  <run_depend>pluginlib</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>tf_conversions</run_depend>

  <run_depend>rospy</run_depend>
  <run_depend>actionlib</run_depend>
  <run_depend>actionlib_msgs</run_depend>
  <run_depend>mdr_behavior_msgs</run_depend>
  <run_depend>moveit_msgs</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>tf</run_depend>
  <run_depend>cob_script_server</run_depend>

  <export>
    <moveit_core plugin="${prefix}/lwr_moveit_ikfast_plugin_description.xml"/>
  </export>
</package>
