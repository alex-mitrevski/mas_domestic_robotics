<?xml version="1.0"?>
<launch>
    <arg name="action_dispatch_topic" default="/kcl_rosplan/action_dispatch"/>
    <node pkg="mdr_pickup_action" type="pickup_client" name="pickup_client" output="screen" ns="mdr_actions">
        <param name="action_name" type="str" value="pickup" />
        <param name="server_name" type="str" value="/mdr_actions/pickup_server" />
        <param name="action_timeout" type="double" value="20" />
        <param name="placing_pose_frame" type="str" value="base_link" />
        <remap from="action_dispatch_topic" to="$(arg action_dispatch_topic)" />
    </node>
</launch>
