<?xml version="1.0"?>
<package>
  <name>mdr_tactile_grasp</name>
  <version>1.1.2</version>
  <description>
  A package that implements reactive grasping based on feedback from
  the tactile sensors of an SDH-2.
  </description>
  <license>GPLv3</license>

  <maintainer email="jose.sanchez.loza@gmail.com">Jose Sanchez</maintainer>
  <maintainer email="robotics@inf.h-brs.de">MAS robotics</maintainer>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>rospy</build_depend>

  <run_depend>rospy</run_depend>
  <run_depend>control_msgs</run_depend>
  <run_depend>schunk_sdh</run_depend>
</package>
